  "description": "PureProxy utilities",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pureproxy/utils.git"
  },
  "bugs": {
    "url": "https://github.com/pureproxy/utils/issues"
  },
  "homepage": "https://github.com/pureproxy/utils#readme",
  "author": "pdparchitect",
  "keywords": [
    "proxy",
    "server",
    "stream",
    "http",
    "https",
    "websocket",
    "websockets"
  ],
